{
    "name": "ensostudio/yii2-fenom",
    "description": "Yii 2 extension for using Fenom template engine",
    "keywords": [
        "yii2",
        "yii",
        "extension",
        "fenom",
        "template"
    ],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Enso studio",
            "email": "info@ensostudio.ru"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "^2.0.30",
        "fenom/fenom": "^2.12.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.20"
    },
    "autoload": {
        "psr-4": {
            "ensostudio\\yii2fenom\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ensostudio\\yii2fenom\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
